<header>
    <span class="siteName">
        <a href="/" title="Return to home page">
            CSE Motors
        </a>
    </span>
    <div class="tools">
        <% if (locals.loggedin === 1) { %>
            <a href="/account/">Welcome, <%= locals.accountData.account_firstname%> <%= locals.accountData.account_lastname %></a>
            <span>|</span>
            <a href="/account/logout/">Log Out</a>
        <% } else { %>
        <a title="Click to log in" href="/account/login">
            Log In
        </a>
        <% } %>
    </div>
</header>
